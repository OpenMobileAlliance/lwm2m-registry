<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.openmobilealliance.org/tech/profiles/LWM2M-v1_1.xsd">
	<Object ObjectType="MODefinition">
		<Name>Thread Network</Name>
		<Description1><![CDATA[This LWM2M object is used to describe and modify Thread network configurations]]></Description1>
		<ObjectID>10483</ObjectID>
		<ObjectURN>urn:oma:lwm2m:x:10483</ObjectURN>
		<LWM2MVersion>1.1</LWM2MVersion>
		<ObjectVersion>1.0</ObjectVersion>
		<MultipleInstances>Single</MultipleInstances>
		<Mandatory>Optional</Mandatory>
		<Resources>
			<Item ID="0">
				<Name>Network Name</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[Thread network name.]]></Description>
			</Item>
			<Item ID="1">
				<Name>PAN ID</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[Thread network Personnal Area Network Identification (PAN Id).]]></Description>
			</Item>
			<Item ID="2">
				<Name>Extended PAN ID </Name>
				<Operations>RW</Operations>
				<MultipleInstances>Multiple</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[Thread network Extended Personal Area Network Identification (Ext PAN Id). ]]></Description>
			</Item>
			<Item ID="3">
				<Name>Passphrase</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[Thread network passphrase.]]></Description>
			</Item>
			<Item ID="4">
				<Name>Master Key</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[Thread network master key value.]]></Description>
			</Item>
			<Item ID="5">
				<Name>Channel</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Integer</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[The communication channel number. IEEE 802.15.4 Channel value.]]></Description>
			</Item>
			<Item ID="6">
				<Name>IpV6 Mesh Local Prefix</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[The IPv6 mesh local prefix.]]></Description>
			</Item>
			<Item ID="7">
				<Name>Max Childrens</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Integer</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>count</Units>
				<Description><![CDATA[The configured maximum number of childrens.]]></Description>
			</Item>
			<Item ID="8">
				<Name>RLOC16 Address</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[The Routing Locator (RLOC16) identifies a Thread interface, based on its location in the network topology.]]></Description>
			</Item>
			<Item ID="9">
				<Name>EUI64</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[Indicates the Extended Unique Identifier (EUI64) of the device.]]></Description>
			</Item>
			<Item ID="10">
				<Name>Extended MAC Address</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[The extended media access control (MAC) address of the device.]]></Description>
			</Item>
			<Item ID="11">
				<Name>IPV6 Addresses</Name>
				<Operations>R</Operations>
				<MultipleInstances>Multiple</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[A list of assigned IP addresses.]]></Description>
			</Item>
		</Resources>
		<Description2 />
	</Object>
</LWM2M>
